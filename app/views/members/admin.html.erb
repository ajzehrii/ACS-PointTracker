<center>

<p id="notice"><%= notice %></p>

<h1>Members</h1>

<table>
  <thead>
    <tr>
      <th>Last</th>
      <th>First</th>
      <th>Email</th>
      <th>Student id</th>
      <th>Admin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.last %></td>
        <td><%= user.first %></td>
        <td><%= user.email %></td>
        <td><%= user.student_id %></td>
        <td><%= user.admin %></td>
        <td><%= link_to 'Edit', edit_user_registration_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Member', new_member_path %>

</center>